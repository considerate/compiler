class IntList {
    var head : Int = 0;
    var tail : IntList = null;
    var set : Boolean = false;
    def cons(h : Int, t : IntList) : IntList = {
        if (set) {
            println("Screw You.");
            this
        }
        else {
            head = h;
            tail = t;
            set = true;
            this
        }
    }
    def head() : Int = {head}
    def tail() : IntList = {tail}
    def toString() : String = {
        this.head() + " :: " + (if (this.tail() == null) "Nil" else this.tail().toString())
    }
}

