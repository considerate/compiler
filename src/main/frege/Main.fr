module Main where
import lexer.Lexer (runLexer, toValidTokens)
import lexer.Punkt0Lexer (token)
import parser.Punkt0Parser (expression, simpleexpression)

main :: [String] -> IO ()
main _
    = getContents >>= \str ->
        let
            tokens = ((runLexer token) . toList) str
            parseTree = (expression.run . toValidTokens) tokens
        in (println . show) tokens
            >> (println . show) parseTree


